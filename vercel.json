{
  "version": 2,
  "buildCommand": "cd App && npm run build",
  "outputDirectory": "App/dist/app/browser",
  "builds": [
    {
      "src": "api/src/vercel-handler.ts",
      "use": "@vercel/node"
    },
    {
      "src": "App/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "cd App && npm run build",
        "distDir": "App/dist/app/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/src/vercel-handler.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
